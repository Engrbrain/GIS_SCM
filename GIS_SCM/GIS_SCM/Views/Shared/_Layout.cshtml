            <script type="text/javascript" src="js/plugins/cropper/cropper.min.js"></script>

            <script type="text/javascript" src="js/plugins/jstree/jstree.min.js"></script>
            <!-- END PAGE PLUGINS -->

            <script type="text/javascript" src="~/Content/js/plugins/moment.min.js"></script>
            <script type="text/javascript" src="~/Content/js/plugins/daterangepicker/daterangepicker.js"></script>
            <!-- END THIS PAGE PLUGINS-->
