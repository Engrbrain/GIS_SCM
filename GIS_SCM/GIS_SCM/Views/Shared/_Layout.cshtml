                        <li><a href="ui-buttons.html"><span class="fa fa-square-o"></span> DC Reorder Analysis</a></li>
                        <li><a href="ui-panels.html"><span class="fa fa-pencil-square-o"></span> Delivery Routing</a></li>
                        <li><a href="ui-typography.html"><span class="fa fa-pencil"></span> Delivery Tracking</a></li>
                        
                    </ul>
                </li>
                
                <li class="xn-title">Master Data Management</li>
                <li class="xn-openable">
                    <a href="#"><span class="fa fa-cogs"></span> <span class="xn-text">Customer Master</span></a>
                    <ul>
                        <li><a href="@Url.Action("index","Customer_Upload")"><span class="fa fa-heart"></span> Upload Customers</a></li>
                        <li><a href="@Url.Action("index","Customers")"><span class="fa fa-cogs"></span>Manage Customers</a></li>

                    </ul>
                </li>

                <li class="xn-openable">
                    <a href="#"><span class="fa fa-cogs"></span> <span class="xn-text">Supplier Master</span></a>
                    <ul>
                        <li><a href="@Url.Action("index","Supplier_Upload")"><span class="fa fa-heart"></span> Upload Supplier</a></li>
                        <li><a href="@Url.Action("index","Suppliers")"><span class="fa fa-cogs"></span>Manage Supplier</a></li>

                    </ul>
                </li>

                <li class="xn-openable">
                    <a href="#"><span class="fa fa-cogs"></span> <span class="xn-text">Material Master</span></a>
                    <ul>
                        <li><a href="@Url.Action("index","Material_Upload")"><span class="fa fa-heart"></span> Upload Material</a></li>
                        <li><a href="@Url.Action("index","Materials")"><span class="fa fa-cogs"></span>Manage Material</a></li>

                    </ul>
                </li>

                <li class="xn-openable">
                    <a href="#"><span class="fa fa-cogs"></span> <span class="xn-text">Plant Master</span></a>
                    <ul>
                        <li><a href="@Url.Action("index","Plant_Upload")"><span class="fa fa-heart"></span> Upload Plant</a></li>
                        <li><a href="@Url.Action("index","Plants")"><span class="fa fa-cogs"></span>Manage Plant</a></li>

                    </ul>
                </li>

                <li class="xn-openable">
                    <a href="#"><span class="fa fa-cogs"></span> <span class="xn-text">Distribution Centers Master</span></a>
                    <ul>
                        <li><a href="@Url.Action("index","DC_Upload")"><span class="fa fa-heart"></span> Upload DC</a></li>
                        <li><a href="@Url.Action("index","DistributionCenters")"><span class="fa fa-cogs"></span>Manage DC</a></li>

                    </ul>
                </li>

                <li class="xn-openable">
                    <a href="#"><span class="fa fa-cogs"></span> <span class="xn-text">Storage Location Master</span></a>
                    <ul>
                        <li><a href="ui-widgets.html"><span class="fa fa-heart"></span> Upload Storage Location</a></li>
                        <li><a href="ui-elements.html"><span class="fa fa-cogs"></span>Manage Storage Location</a></li>

                    </ul>
                </li>
               
               
            </ul>
