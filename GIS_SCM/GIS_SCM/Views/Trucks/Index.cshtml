@model IEnumerable<GIS_SCM.Models.Truck>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TruckNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TruckModel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TruckColor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastParkedLongitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastParkedLatitude)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TruckNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TruckModel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TruckColor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastParkedLongitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastParkedLatitude)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
